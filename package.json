{
  "name": "support-requests",
  "version": "0.1.1",
  "description": "A GitHub App built with Probot that comments on and closes issues labeled as support requests.",
  "author": "Armin Sebastian",
  "license": "MIT",
  "repository": "https://github.com/dessant/support-requests",
  "scripts": {
    "start": "probot run ./src/index.js",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version && yarn run push"
  },
  "dependencies": {
    "probot": "^3.0.2"
  },
  "devDependencies": {
    "npm-check-updates": "^2.13.0",
    "standard-version": "^4.2.0"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  }
}
